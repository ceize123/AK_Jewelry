\n\n// scoll to element\n  const aboutSection = useRef(null);\n\n  const gotoAboutSection = () =>\n    window.scrollTo({\n      top: aboutSection.current.offsetTop,\n      behavior: 'smooth'\n    });\n\n// aos\n  useEffect(() => {\n    Aos.init({\n      duration: 1000,\n      once: true,\n      offsetTop: 0,\n      delay: 500,\n    });\n  }, []);\n\n  return (\n    <>\n      <header>\n        <div className='sloganSec'>\n          <div>\n            <img src={bling} alt=\"header\"/>\n            {/* <div className='glow'>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n              <div className='darkGrayLine'></div>\n            </div> */}\n          </div>\n          <div data-aos='fade-up' className='slogan'>\n            <p>喚起你心中的藝術家，</p>\n            <p>開啟閃亮亮斜槓人生</p>\n          </div>\n          <div data-aos='fade-up' data-aos-delay='800' className='getIn' onClick={gotoAboutSection}>\n            <p><i class=\"las la-long-arrow-alt-right\"></i>進入珠寶世界</p>\n          </div>\n        </div>\n      </header>\n      <main>\n        <div className='aboutSec' ref={aboutSection}>\n          <p data-aos='fade-up' data-aos-offset='120' data-aos-delay='0'>About</p>\n          <span data-aos='fade-up' data-aos-offset='120' data-aos-delay='0'>ANDREA老師畢業於紐約FIT珠寶設計學系，\n          畢業後立即進入了紐約一線的珠寶設計師品牌執業，\n          為好萊塢明星提供客製化與紅毯珠寶設計，有多年業界經驗。\n          <br/>\n          <br/>\n          因現居北美洲，ANDREA老師一年只會回來開2次課程...</span>\n          <button data-aos='fade-up' id='moreBtn' data-aos-offset='0' data-aos-delay='0'>More</button>\n        </div>\n        <div className='lectureInfo'>\n          <img src={abstract2} alt=\"Jewelry\"/>\n          <p data-aos='fade-up'>近期開課資訊</p>\n        </div>\n        <div className='dateInfo'>\n          <LessonSection handText='初階手繪珠寶設計' digitalText='數位珠寶設計'/>\n        </div>\n        <div className='tellMeTime'>\n          <p data-aos='fade-up' data-aos-anchor-placement='top-center' data-aos-duration='0'>目前開課時間都無法配合<span id='comma'>，</span><span>怎麼辦？</span></p>\n          <p data-aos='fade-up' data-aos-anchor-placement='top-center' data-aos-duration='0'>別擔心！<span>告訴我們你可以的時間</span></p>\n          <button data-aos='fade-up' data-aos-anchor-placement='top-center' data-aos-duration='0'>填寫</button>\n        </div>\n      </main>\n    </>\n  );\n}\n\n\nexport default LandingPage;\n","/Users/ceize/Desktop/ak_jewelry/src/components/diamondContent.js",[],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"70","line":31,"column":9,"nodeType":"68","messageId":"69","endLine":31,"endColumn":24},{"ruleId":"66","severity":1,"message":"71","line":2,"column":10,"nodeType":"68","messageId":"69","endLine":2,"endColumn":14},"no-native-reassign",["72"],"no-negated-in-lhs",["73"],"no-unused-vars","'useState' is defined but never used.","