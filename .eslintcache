s.css';\n\nfunction About() {\n\n  useEffect(() => {\n    Aos.init({\n      duration: 1000,\n      once: true,\n      offsetTop: 100,\n      delay: 200,\n    });\n  }, []);\n\n  return (\n    <>\n      <div id='aboutPage'>\n        <HeaderContent title='About' sub='師資介紹'/>\n        <main>\n          <ListsWithDiamond className='andrea' title='Andrea Ko' listName={aboutPageAndrea} />\n          <ListsWithDiamond className='whyChose' title='為什麼選Andrea Ko珠寶教室?' listName={whyChose} />\n          <div className='designSec'>\n            <h3 data-aos='fade-up'><span>Andrea</span>的設計</h3>\n            <span className='designSecSpan' data-aos='fade-up'>手飾和耳環為ANDREA設計 (版權所有洛琳·施華滋)</span>\n            <div>\n              <img src={aboutImg1} alt=\"明星穿戴照1\" data-aos='fade-up' />\n              <img src={aboutImg2} alt=\"明星穿戴照2\" data-aos='fade-up' />\n              <img src={aboutImg3} alt=\"明星穿戴照3\" data-aos='fade-up' />\n              <img src={aboutImg4} alt=\"明星穿戴照4\" data-aos='fade-up' />\n              <img src={aboutImg5} alt=\"明星穿戴照5\" data-aos='fade-up' />\n              <img src={aboutImg6} alt=\"明星穿戴照6\" data-aos='fade-up' />\n              <img src={aboutImg7} alt=\"明星穿戴照7\" data-aos='fade-up' />\n              <img src={aboutImg8} alt=\"明星穿戴照8\" data-aos='fade-up' />\n            </div>\n          </div>\n          <div className='getCase'>\n            <h3 data-aos='fade-up' data-aos-offset='0' data-aos-delay='500'>獨立接案設計師<span>Freelance</span></h3>\n            <div className='freelanceImg'>\n              <img src={getCase} alt=\"Freelance\"/>\n              <div className='fadeIn' data-aos='fade-up' data-aos-offset='0' data-aos-duration='1800' data-aos-delay='600'>\n                <span>\n                  ANDREA KO 定居多倫多後成立自有工作室，轉型專案設計師，\n                  除了教課之外Andrea也提供客製化珠寶訂製服務，無論是婚戒、各種紀念日、\n                  節慶禮物或者企業品牌的合作項目，都歡迎來訊洽談。\n                  <br/>\n                  <br/>\n                  近期開始籌劃銀飾珠寶系列DRIFTSILVER。DRIFTSILVER 由兩個字組成，\n                  其中SILVER意指該系列以銀飾為主打， DRIFT則象徵了設計師珠寶應該自由奔放，\n                  不受框架拘束的精神。\n                </span>\n                <button className='longBtn' id='cooperateBtn'><DelayLink delay={400} to='/freelance'>企業合作/個人訂製</DelayLink></button>\n              </div>\n            </div>\n          </div>\n          <div className='aboutPageLastBtn' data-aos='fade-up' data-aos-offset='0' data-aos-delay='400'>\n            <button className='longBtn'><DelayLink delay={400} to='/#lectureInfoSection'>近期課程資訊</DelayLink></button>\n            <button className='longBtn'><DelayLink delay={400} to='/porfolio'>作品集</DelayLink></button>\n          </div>\n        </main>\n      </div>\n    </>\n  );\n}\n\n\nexport default About;\n","/Users/ceize/Desktop/ak_jewelry/src/components/Blog.js",["82"],"/Users/ceize/Desktop/ak_jewelry/src/components/Freelance.js",["83","84"],"/Users/ceize/Desktop/ak_jewelry/src/components/Porfolio.js",["85"],"/Users/ceize/Desktop/ak_jewelry/src/components/Footer.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/Navbar.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/LandingPage.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/pagesContent.js",["86"],"/Users/ceize/Desktop/ak_jewelry/src/components/diamondListAndArticleContent.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/ArticleContent.js",["87"],"import React, { useState, useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport DelayLink from 'react-delay-link';\nimport { article } from './diamondListAndArticleContent.js';\nimport { useParams } from 'react-router-dom';\nimport { RandomBlogArticle } from './pagesContent.js';\nimport Aos from 'aos';\nimport 'aos/dist/aos.css';\n\nfunction Articles() {\n  const { title } = useParams();\n  let [currentImg, setCurrentImg] = useState(0);\n\n  const [touchStart, setTouchStart] = useState(0);\n  const [touchEnd, setTouchEnd] = useState(0);\n\n  function handleTouchEnd() {\n    if (touchStart - touchEnd > 5 & currentImg < 2) {\n        // do your stuff here for left swipe\n        setCurrentImg( currentImg + 1 );\n    }\n    if (touchStart - touchEnd < -5 & currentImg > 0 ) {\n        // do your stuff here for right swipe\n        setCurrentImg( currentImg - 1 );\n    }\n  }\n\n  useEffect(() => {\n    Aos.init({\n      duration: 1000,\n      once: true,\n      offsetTop: 100,\n      delay: 200,\n    });\n  }, []);\n\n  return (\n    <>\n    <div id='Articles'>\n      <main>\n      {/* 讓內容可以藉由title做篩選 */}\n        {article.filter(content => content.title === title).map((content, key) =>(\n          <div key={key}>\n            <div className='imgsSecWithRadio'\n                onTouchStart={(e) => setTouchStart(e.targetTouches[0].clientX)}\n                onTouchMove={(e) => setTouchEnd(e.targetTouches[0].clientX)}\n                onTouchEnd={handleTouchEnd}\n                >\n              <div id='articleImgs' style={{left: currentImg * -100 + 'vw' }}>\n                <img src={content.img} alt=\"img1\"/>\n                <img src={content.img2} alt=\"img2\"/>\n                <img src={content.img3} alt=\"img3\"/>\n              </div>\n              <div className='carouselInput'>\n                <ul>\n                  <li className={currentImg === 0 ? 'selected' : ''} onClick={() => {\n                    setCurrentImg(currentImg = 0);\n                    }}></li>\n                  <li className={currentImg === 1 ? 'selected' : ''} onClick={() => {\n                    setCurrentImg(currentImg = 1);\n                    }}></li>\n                  <li className={currentImg === 2 ? 'selected' : ''} onClick={() => {\n                    setCurrentImg(currentImg = 2);\n                    }}></li>\n                </ul>\n              </div>\n            </div>\n            <h5 data-aos='fade-up' data-aos-delay='500'>{content.title}</h5>\n            <p data-aos='fade-up'>{content.content}</p>\n          </div>\n        ))}\n        <div className='articleBtn' data-aos='fade-up'>\n          <button className='longBtn'><DelayLink delay={400} to='/'>近期課程資訊</DelayLink></button>\n          <button className='longBtn'><DelayLink delay={400} to='/porfolio'>作品集</DelayLink></button>\n        </div>\n        <h5 data-aos='fade-up'>其他人也看過</h5>\n        <RandomBlogArticle />\n      </main>\n    </div>\n    </>\n  );\n}\n\nexport default Articles;\n","/Users/ceize/Desktop/ak_jewelry/src/components/FAQ.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/CourseBeginnerHand.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/CourseReview.js",["88"],"import React from 'react';\nimport DelayLink from 'react-delay-link';\nimport {\n  HeaderContent,\n  ListsWithDiamond,\n  ParallaxCricle,\n  SignUp,\n  TellMeTime\n} from './pagesContent.js';\nimport { reviewSuit, reviewLearnWhat } from './diamondListAndArticleContent.js';\n\nfunction CourseReview() {\n  return (\n    <>\n      <div id='courseReviewPage'>\n        <HeaderContent title='Rreview For Free' sub='免費複習課程'/>\n        <main>\n          <ListsWithDiamond className='reviewSuitFor' title='適合對象...' listName={reviewSuit} />\n          <ParallaxCricle text={['歡迎大家回來畫畫珠寶，', <br/>, '談天說地！']}/>\n          <ListsWithDiamond className='reviewLearnWhat' title='你可以學到...' listName={reviewLearnWhat} />\n          <ParallaxCricle text='任何珠寶相關問題不怕你問！'/>\n          <div className='classInfo' data-aos='fade-up' data-aos-delay='600'>\n            <h3>課程資訊</h3>\n            <h4>上課日期 &lt;2021&gt;</h4>\n            <span>\n            籌備中，敬請期待！<br/>\n            Andrea 老師一年回台灣開課兩次，\n            等確立回台開課時間會再通知同學，\n            歡迎留下聯絡資訊，才不會漏接開課重要通知喔！\n            </span>\n            <button className='shortBtn'>開課通知我</button>\n            <h4>費用 ＆ 工具</h4>\n            <span>免費，無需準備材料</span>\n            <h4>上課地點</h4>\n            <span>台北市，近西門捷運站 (走路約5分鐘)</span>\n          </div>\n          <ParallaxCricle text='免費資源！要把握機會唷！'/>\n          <SignUp />\n          <TellMeTime />\n        </main>\n      </div>\n    </>\n  )\n}\n\nexport default CourseReview;\n","/Users/ceize/Desktop/ak_jewelry/src/components/Course3D.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/CourseDigital.js",[],"/Users/ceize/Desktop/ak_jewelry/src/components/CourseAdvancedHand.js",["89"],"import React, { useEffect } from 'react';\nimport {\n  HeaderContent,\n  ListsWithDiamond,\n  ParallaxCricle,\n  YouCanLearn,\n  SignUp,\n  TellMeTime\n} from './pagesContent.js';\nimport {advancedHandSuit, advancedLearnWhat, whyChose} from './diamondListAndArticleContent.js';\n\nfunction CourseAdvancedHand() {\n  return (\n    <>\n      <div id='courseAdvancedHandPage'>\n        <HeaderContent title='Advanced' sub='進階手繪珠寶設計'/>\n        <main>\n        <ListsWithDiamond className='advancedSuitFor' title='適合對象...' listName={advancedHandSuit} />\n        <ParallaxCricle text={['歡迎你!', <br/>, 'Andrea 將手把手的帶你一步一步進入珠寶的世界！']}/>\n        <ListsWithDiamond className='advancedLearnWhat' title='你可以學到...' listName={advancedLearnWhat} />\n        <ParallaxCricle text={['持續精進自己，', <br/>, '任何寶石畫法都難不倒你！']}/>\n        <ListsWithDiamond className='whyChose' title='為什麼選Andrea Ko珠寶教室?' listName={whyChose} />\n        <ParallaxCricle text='我們重視與產業接軌以及學生的設計發想能力，打好基礎，才能走得更長遠。'/>\n        <YouCanLearn />\n        <ParallaxCricle text='一步一腳印，上完課後你可以看到自己的成長！'/>\n        <div className='classInfo' data-aos='fade-up' data-aos-delay='600'>\n          <h3>課程資訊</h3>\n          <h4>上課日期 &lt;2021&gt;</h4>\n          <span>\n          籌備中，敬請期待！<br/>\n          Andrea 老師一年回台灣開課兩次，\n          等確立回台開課時間會再通知同學，\n          歡迎留下聯絡資訊，才不會漏接開課重要通知喔！\n          </span>\n          <button className='shortBtn'>開課通知我</button>\n          <h4>費用 ＆ 工具</h4>\n          <span>NT$26000<br/>\n            課上提供上課用具，毋須自備材料。\n            另贈兩組珠寶製圖規版。\n          </span>\n          <h4>上課地點</h4>\n          <span>台北市，近西門捷運站 (走路約5分鐘)</span>\n          <h4>其他注意事項</h4>\n          <span>如繳費完畢，臨時有事無法前來參加課程，不提供退費，但同學可以自行轉讓給其他同學。</span>\n        </div>\n        <ParallaxCricle text